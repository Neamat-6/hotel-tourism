"<t t-if=\"line.quantity_done\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Attiert:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Lieferung:* <t t-out=\"line.quantity_done or ''\"/>\n"
"</t>\n"
"<t t-else=\"\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Attiert:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Lieferung:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"</t>\n"
"</t>\n"
"</t>\n"
"% endif\n"
"<t t-if=\"object.company_id.inventory_signature and object.env.user.sign\">\n"
