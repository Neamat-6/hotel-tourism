#. module: sh_whatsapp_integration_api
#: model:mail.template,body_html:sh_whatsapp_integration_api.email_template_edi_invoice_custom_chat_api
msgid ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"<t t-if=\"object.company_id.invoice_order_information_in_message\">\n"
"Dear <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"<t t-if=\"object.name and object.state != 'draft'\">\n"
"Here is the your invoice *<t t-out=\"object.name or ''\"/>*.</t>\n"
"\n"
"<t t-else=\"\">\n"
"Here is your invoice *<t t-out=\"object.name or ''\"/>* amounting in *<t t-out=\"object.amount_total or ''\"/> <t t-out=\"object.currency_id.symbol or ''\"/>* from ${object.company_id.name}<t t-out=\"object.company_id.name or ''\"/>\n"
"\n"
"</t>\n"
"<t t-if=\"object.payment_state == 'paid'\">\n"
"This invoice is already paid.</t>\n"
"\n"
"<t t-else=\"\">\n"
"Please remit payment at your earliest convenience.\n"
"\n"
"</t>\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_product_detail_in_message\">\n"
"Following is your invoice details.\n"
"\n"
"<t t-foreach=\"object.invoice_line_ids\" t-as=\"line\">\n"
"*<t t-out=\"line.product_id.name or ''\"/>*\n"
"*Qty:* <t t-out=\"line.quantity or ''\"/>\n"
"*Price:* <t t-out=\"line.price_unit or ''\"/><t t-out=\"object.currency_id.symbol or ''\"/>\n"
"________________________\n"
"\n"
"</t>\n"
"*Total Amount:* <t t-out=\"object.amount_total or ''\"/><t t-out=\"object.currency_id.symbol or ''\"/>\n"
"\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/>\n"
"</t> \n"
"\t\t\t</p>\n"
"\n"
"\t\t"
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"<t t-if=\"object.company_id.invoice_order_information_in_message\">\n"
"Estimado <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"<t t-if=\"object.name and object.state != 'draft'\">\n"
"Aquí está la factura *<t t-out=\"object.name or ''\"/>*.</t>\n"
"\n"
"<t t-else=\"\">\n"
"Aquí está su factura *<t t-out=\"object.name or ''\"/>* que equivale a *<t t-out=\"object.amount_total or ''\"/> <t t-out=\"object.currency_id.symbol or ''\"/>* desde ${object.company_id.name}<t t-out=\"object.company_id.name or ''\"/>\n"
"\n"
"</t>\n"
"<t t-if=\"object.payment_state == 'paid'\">\n"
"Esta factura ya está pagada.</t>\n"
"\n"
"<t t-else=\"\">\n"
"Por favor remita el pago a su más pronta conveniencia.\n"
"\n"
"</t>\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_product_detail_in_message\">\n"
"A continuación se muestran los detalles de su factura.\n"
"\n"
"<t t-foreach=\"object.invoice_line_ids\" t-as=\"line\">\n"
"*<t t-out=\"line.product_id.name or ''\"/>*\n"
"*Cantidad:* <t t-out=\"line.quantity or ''\"/>\n"
"*Precio:* <t t-out=\"line.price_unit or ''\"/><t t-out=\"object.currency_id.symbol or ''\"/>\n"
"________________________\n"
"\n"
"</t>\n"
"*Cantidad total:* <t t-out=\"object.amount_total or ''\"/><t t-out=\"object.currency_id.symbol or ''\"/>\n"
"\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/>\n"
"</t> \n"
"\t\t\t</p>\n"
"\n"
"\t\t"

#. module: sh_whatsapp_integration_api
#: model:mail.template,body_html:sh_whatsapp_integration_api.mail_template_data_payment_receipt_custom_chat_api
msgid ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"Dear <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"Here is the Payment *<t t-out=\"object.name or ''\"/>* from <t t-out=\"object.company_id.name or ''\"/>\n"
"<t t-if=\"object.company_id.invoice_order_information_in_message\">\n"
"\n"
"Following is your Payment details.\n"
"\n"
"<t t-if=\"object.payment_type == 'inbound'\">\n"
"We received payment of *<t t-out=\"format_amount(object.amount, object.currency_id) or ''\"/>*\n"
"\n"
"Thank You.\n"
"\n"
"</t>\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/>\n"
"\n"
"</t>\n"
"\t\t\t</p>\n"
"\t\t"
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"Estimado <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"Aquí está el pago. *<t t-out=\"object.name or ''\"/>* desde <t t-out=\"object.company_id.name or ''\"/>\n"
"<t t-if=\"object.company_id.invoice_order_information_in_message\">\n"
"\n"
"A continuación se muestran sus detalles de pago.\n"
"\n"
"<t t-if=\"object.payment_type == 'inbound'\">\n"
"Recibimos el pago de *<t t-out=\"format_amount(object.amount, object.currency_id) or ''\"/>*\n"
"\n"
"Gracias.\n"
"\n"
"</t>\n"
"</t>\n"
"<t t-if=\"object.company_id.invoice_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/>\n"
"\n"
"</t>\n"
"\t\t\t</p>\n"
"\t\t"

#. module: sh_whatsapp_integration_api
#: model:mail.template,body_html:sh_whatsapp_integration_api.mail_template_data_stock_picking_custom_chat_api
msgid ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"Dear <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"Here is the Stock Transfer *<t t-out=\"object.name or ''\"/>* from <t t-out=\"object.company_id.name or ''\"/>\n"
"<t t-if=\"object.company_id.inventory_information_in_message\">\n"
"\n"
"Following is your order details.\n"
"\n"
"<t t-foreach=\"object.move_ids_without_package\" t-as=\"line\">\n"
"<t t-if=\"line.quantity_done\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Qty:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Delivery Qty:* <t t-out=\"line.quantity_done or ''\"/>\n"
"</t>\n"
"<t t-else=\"\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Qty:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Delivery Qty:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"</t>\n"
"</t>\n"
"</t>\n"
"% endif\n"
"<t t-if=\"object.company_id.inventory_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/> %0A%0A\n"
"</t>\n"
"\t\t\t</p>\n"
"\t\t"
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<p>\n"
"Estimado <t t-out=\"object.partner_id.name or ''\"/>,\n"
"\n"
"Aquí está la transferencia de acciones *<t t-out=\"object.name or ''\"/>* desde <t t-out=\"object.company_id.name or ''\"/>\n"
"<t t-if=\"object.company_id.inventory_information_in_message\">\n"
"\n"
"Siguiendo son los detalles de su pedido.\n"
"\n"
"<t t-foreach=\"object.move_ids_without_package\" t-as=\"line\">\n"
"<t t-if=\"line.quantity_done\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Cantidad:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Cantidad de entrega:* <t t-out=\"line.quantity_done or ''\"/>\n"
"</t>\n"
"<t t-else=\"\">\n"
"*<t t-out=\"line.name or ''\"/>*\n"
"*Cantidad:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"*Cantidad de entrega:* <t t-out=\"line.product_uom_qty or ''\"/>\n"
"</t>\n"
"</t>\n"
"</t>\n"
"% endif\n"
"<t t-if=\"object.company_id.inventory_signature and object.env.user.sign\">\n"
"<t t-out=\"object.env.user.sign or ''\"/> %0A%0A\n"
"</t>\n"
"\t\t\t</p>\n"
"\t\t"

