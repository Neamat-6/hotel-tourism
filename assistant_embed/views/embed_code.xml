<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="backend_assistant_embed" inherit_id="web.layout" name="Backend Assistant Embed Code">
        <xpath expr="//head" position="inside">
            <t t-if="env['ir.config_parameter'].sudo().get_param('assistant_embed.code')">
                <t t-raw="env['ir.config_parameter'].sudo().get_param('assistant_embed.code')"/>
            </t>
        </xpath>
    </template>
</odoo>