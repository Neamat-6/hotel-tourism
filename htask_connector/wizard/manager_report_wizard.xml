<odoo>
	<data>
		<record id="view_daily_revenues_manager_report_wizard" model="ir.ui.view">
            <field name="name">manager.report.wizard.form</field>
            <field name="model">manager.report</field>
            <field name="arch" type="xml">
                <form string="Manager Report">
                    <group>
                        <field name="date"/>
                    </group>
                    <footer>
                        <button name="generate_manager_report" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>