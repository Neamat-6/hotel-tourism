<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_menu_form_inherit" model="ir.ui.view">
        <field name="name">ir.ui.menu.form.inherit</field>
        <field name="model">ir.ui.menu</field>
        <field name="inherit_id" ref="base.edit_menu_access"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='web_icon_data']" position="after">
                <field name="os_web_icon_font" invisible="0"/>
                <field name="os_web_icon_color" invisible="0"/>
                <field name="os_shape_color" invisible="0"/>
                <field name="os_image_icon" invisible="0"/>
                <field name="os_is_changed" invisible="0"/>
            </xpath>
        </field>
    </record>
</odoo>
