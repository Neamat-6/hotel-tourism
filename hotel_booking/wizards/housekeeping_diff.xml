<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="housekeeping_diff_view_form" model="ir.ui.view">
            <field name="name">housekeeping_diff_view_form</field>
            <field name="model">housekeeping.diff</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="room_ids" widget="many2many_tags" readonly="1" force_save="1"/>
                    </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>