<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data noupdate="1">

        <record id="seq_travel_company_code" model="ir.sequence">
            <field name="name">Travel Company Sequence</field>
            <field name="code">travel.company.sequence</field>
            <field name="prefix"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hotel_booking" model="ir.sequence">
            <field name="name">Hotel Booking</field>
            <field name="code">hotel.booking.ref</field>
            <field name="prefix">BK</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hotel_work_order" model="ir.sequence">
            <field name="name">Hotel Work Order</field>
            <field name="code">hotel.work.order.sequence</field>
            <field name="prefix">WO</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_booking_folio" model="ir.sequence">
            <field name="name">Folio Sequence</field>
            <field name="code">booking.folio.sequence</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>


</odoo>