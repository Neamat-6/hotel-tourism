<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="umraheasy_rate_plan_view_form" model="ir.ui.view">
            <field name="name">umraheasy_rate_plan_view_form</field>
            <field name="model">hotel.rate.plan</field>
            <field name="inherit_id" ref="hotel_booking.rate_plan_view_form"/>
