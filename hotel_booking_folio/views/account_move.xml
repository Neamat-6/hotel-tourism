<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="folio_view_account_move_form" model="ir.ui.view">
        <field name="name">folio_view_account_move_form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@id='header_left_group']" position="inside">
                <field name="booking_folio_id" readonly="1" force_save="1"/>
<!--                <field name="folio_id" readonly="1" force_save="1"/>-->
                <field name="folio_room_id" readonly="1" force_save="1"/>
            </xpath>
        </field>
    </record>
</odoo>