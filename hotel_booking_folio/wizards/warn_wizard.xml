<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="warn_wizard_view_form" model="ir.ui.view">
        <field name="name">warn.wizard.view.form</field>
        <field name="model">warn.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <h4>
                            <field name="message" readonly="1"/>
                        </h4>
                    </group>
                </sheet>
                <footer>
                    <button name='action_confirm' type='object' string="OK" class="oe_highlight"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z" />
                </footer>
            </form>
        </field>
    </record>

</odoo>