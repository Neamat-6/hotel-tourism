    <record model="ir.actions.server" id="action_change_user_password">
          <field name="name">Change Password New</field>
          <field name="model_id" ref="base.model_res_users"/>
          <field name="binding_type">action</field>
            <field name="binding_model_id" ref="base.model_res_users"/>
          <field name="state">code</field>
          <field name="code">
for record in records:
  if record.partner_id and record.partner_id.passport_no:
    new_password = record.partner_id.passport_no
    record.sudo().write({'password': record.partner_id.passport_no})
          </field>
        </record>

