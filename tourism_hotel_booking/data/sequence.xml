<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data noupdate="1">

        <record id="seq_hotel_booking" model="ir.sequence">
            <field name="name">Hotel Booking</field>
            <field name="code">tourism.hotel.booking.ref</field>
            <field name="prefix">TBK</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hotel_booking_transportation" model="ir.sequence">
            <field name="name">Hotel Booking Transportation</field>
            <field name="code">hotel.booking.transportation</field>
            <field name="prefix">BKT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_reservation_package" model="ir.sequence">
            <field name="name">Reservation Package</field>
            <field name="code">reservation.package.ref</field>
            <field name="prefix">RP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>


</odoo>