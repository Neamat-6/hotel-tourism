<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="InternalDashboard" >

        <div class="container">
            <header class="blog-header py-3">
                <div class="row flex-nowrap justify-content-between align-items-center">
                  <div class="col-4 pt-1"></div>
                  <div class="col-4 text-center">
                    <h1 class="blog-header-logo text-dark" href="#">لوحة القيادة للأشراف الداخلي</h1>
                  </div>
                  <div class="col-4 d-flex justify-content-end align-items-center"></div>
                </div>
                <div class="row flex-nowrap justify-content-between align-items-center mt-5">
                  <div class="col-1 pt-1"></div>
                  <div class="col-10 text-center form-group">
                      From :-
                      <input class="inner_select" type="date" id="start_date" name="start_date"  placeholder="From" />
                      To :-
                      <input class="inner_select" type="date" placeholder="To" id="end_date" name="end_date"/>
					  <input type="text" id="company_input" name="company_input" t-att-value="widget.company" class="d-none"/>
                  </div>
                  <div class="col-1 d-flex justify-content-end align-items-center">
                  </div>
                </div>
              </header>


			<section id="tabs" class="project-tab">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <nav>
                            <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                                <a class="nav-item nav-link active btn-outline-success me-2" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">
									1970-01-01
								</a>
                            </div>
                        </nav>
                        <div class="tab-content" id="nav-tabContent">
                        </div>
                    </div>
                </div>
            </div>
        </section>
        </div>
    </t>
</templates>